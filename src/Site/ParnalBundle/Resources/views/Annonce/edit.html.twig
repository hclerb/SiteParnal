<form id="annnonceupdate"  action="{{ path('annonce_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit" id="bpedit">Modification</button>
    </p>
</form>
<script type="text/javascript">
$(function(){
 $("#site_parnalbundle_annoncetype_debut").datepicker();
 $("#site_parnalbundle_annoncetype_fin").datepicker();
 $("#bpedit").button().click(function(event){ 
     var Data = $("#annnonceupdate").serialize();
         $.ajax({
              url : "{{ path('annonce_update', { 'id': entity.id }) }}",
              type:"POST",
              datatype:"text",
              data : Data,
              success: function(reponse) {
                       if (reponse==1) {
                         $("#zoneanno").load("{{ path('annonce') }}"); 
                       }
                    }, 
              error : function(reponse) {
                      alert(reponse);
              }
          }); 
          return false;
   });
});
</script>
{{ tinymce_init() }}
